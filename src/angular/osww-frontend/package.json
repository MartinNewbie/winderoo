{
  "name": "osww-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build-arduino": "ng build --aot --build-optimizer --optimization --progress --output-hashing none && npm run compress-artifacts && npm run move-artifacts",
    "compress-artifacts": "gzip-all \"dist/osww-frontend/*.{css,js}\" && rimraf dist/osww-frontend/*.js && rimraf dist/osww-frontend/*.css",
    "move-artifacts": "move-file ./dist/* ../../platformio/osww-server/data/www",
    "bundle-analyze": "ng build --stats-json --output-hashing none && webpack-bundle-analyzer dist/osww-frontend/stats.json",
    "serve-artifacts": "lite-server --baseDir=\"dist/osww-frontend\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.0",
    "@angular/common": "^14.2.0",
    "@angular/compiler": "^14.2.0",
    "@angular/core": "^14.2.0",
    "@angular/forms": "^14.2.0",
    "@angular/platform-browser": "^14.2.0",
    "@angular/platform-browser-dynamic": "^14.2.0",
    "@angular/router": "^14.2.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.9",
    "@angular/cli": "~14.2.9",
    "@angular/compiler-cli": "^14.2.0",
    "@types/jasmine": "~4.0.0",
    "delete": "^1.1.0",
    "gzip-all": "^1.0.0",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "lite-server": "^2.5.4",
    "move-file-cli": "^3.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "~8.3.0",
    "typescript": "~4.7.2"
  }
}
